            <li><a href="../static/about.html">About</a></li>
            <input type="text" name="Search" id="search-box" placeholder="Search">
        </ul>

    </nav>
    <section>

        <div class="container" id="login-social">
            <a href="www.Facebook.com"><img src="../static/fb.png" alt=""></a>
            <a href="www.Twitter.com"><img src="../static/twitter.png" alt=""></a>
            <a href="www.Instagram.com"><img src="../static/instagram.png" alt=""></a>
            <input type="button" value="Login">
            <input type="button" value="Signup">
